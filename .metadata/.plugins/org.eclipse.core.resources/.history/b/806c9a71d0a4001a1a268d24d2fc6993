package javaEclipseTest;

import java.time.*;
import java.time.format.*;

public class IndexClass {
	
	/*enum Level{
		LOW,
		MEDIUM,
		HIGH
	} */
	
	public static void main(String[] args) {
		
		//date and time java api
		LocalDateTime currentDateTime = LocalDateTime.now();
		DateTimeFormatter chngFormat = DateTimeFormatter.ofPattern("MM-dd-yyyy \nHH:mm:ss");
	
		System.out.print(currentDateTime.format(chngFormat).toString());
	}
}
