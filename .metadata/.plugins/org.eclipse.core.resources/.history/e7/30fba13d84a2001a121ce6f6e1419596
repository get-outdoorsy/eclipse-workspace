public class IndexClass {

	/**
	 * @param args
	 */
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Person cred2 = new Person();
		System.out.print("Old record.\nFirst Name: "+cred2.frstName+"\nLast Name: "+cred2.lName+"\nAge: "+cred2.age);
		
		UserInput data = new UserInput();
		
			
		Person cred = new Person(data.userFname, data.userLname, data.userAge);
		System.out.print("\nUpdate successful!\nFirst Name: "+cred.frstName+"\nLast Name: "+cred.lName+"\nAge: "+cred.age);
		
	
		
	}
}
